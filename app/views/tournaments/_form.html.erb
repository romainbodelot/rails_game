<%= form_for(@tournament) do |f| %>
<ul>
  <% @tournament.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>
<p>
  <%= f.label :title %><br/>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :address %><br/>
  <%= f.text_area :address %>
</p>
<p>
  <%= f.label :date %><br/>
  <%= f.date_select :date %>
</p>
<p>
  <%= f.label :game_list %><br/>
  <%= f.text_field :game_list %>
</p>
<p>
  <%= f.label :max_player %><br/>
  <%= f.number_field :max_player %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
